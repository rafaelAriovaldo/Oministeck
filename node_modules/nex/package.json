{
  "name": "nex",
  "description": "Simplify management of local npm and node.js development resources and processes, such as symlinks, global dependencies, etc. It's like grunt or gulp, but with 100x less code and a flatter learning curve.",
  "version": "2.0.22",
  "homepage": "https://github.com/xtuple/nex",
  "author": {
    "name": "Travis Webb <me@traviswebb.com>"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/xtuple/nex"
  },
  "bugs": {
    "url": "https://github.com/xtuple/nex/issues"
  },
  "license": "MIT",
  "scripts": {
    "test": "mocha test/ -R spec -b"
  },
  "bin": {
    "nex": "index.js"
  },
  "preferGlobal": true,
  "dependencies": {
    "colors": "^0.6.2",
    "commander": "^2.2.0",
    "harmonious": "^0.5.4",
    "lodash": "^2.4.1",
    "nex-api": "^2.0.1",
    "npmlog": "^0.1.1",
    "npmlog-file": "^0.1.1",
    "rimraf": "^2.2.8"
  },
  "bundledDependencies": [
    "colors",
    "commander",
    "lodash",
    "npmlog",
    "npmlog-file",
    "rimraf"
  ],
  "devDependencies": {
    "mocha": "~1.18.2",
    "nex-test": "^0.5.0"
  },
  "globalDependencies": {
    "n": "^1.2.9"
  },
  "linkDependencies": {
    "nex-test": "test"
  },
  "nex": [
    "engines",
    "globalDependencies",
    "linkDependencies"
  ],
  "engineStrict": false,
  "engines": {
    "node": ">=0.11.12"
  },
  "keywords": [
    "npm",
    "nexus",
    "nex",
    "ide",
    "node",
    "build",
    "tool",
    "development",
    "dependencies",
    "globalDependencies",
    "peerDependencies",
    "linkDependencies",
    "systemDependencies",
    "symlink",
    "symlinks",
    "link",
    "management",
    "environment",
    "workspace",
    "private",
    "proprietary"
  ]
